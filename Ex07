class Program
{
    static void Main()
    {
        for (int numero = 1; numero <= 20; numero++)
        {
            Console.WriteLine($"Tabuada do número {numero}:");

            for (int i = 1; i <= 10; i++)
            {
                Console.WriteLine($"{numero} x {i} = {numero * i}");
            }

            Console.WriteLine("Pressione qualquer tecla para continuar para a próxima tabuada...");
            Console.ReadKey();
            Console.WriteLine();
        }
    }
}
