class Program
{
    static void Main()
    {
        int num1 = 1;
        int num2 = 1;

        Console.WriteLine($"Os trinta primeiros valores da s√©rie de Fibonacci:");

        for (int i = 0; i < 30; i++)
        {
            Console.WriteLine(num1);

            int proximoNumero = num1 + num2;

            num1 = num2;
            num2 = proximoNumero;
        }
    }
}
